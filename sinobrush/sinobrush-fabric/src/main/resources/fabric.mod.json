{
    "schemaVersion": 1,
    "id": "${modid}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${sinoseries_authors}"
    ],
    "contact": {
        "homepage": "${sinoseries_url}",
        "sources": "${sinoseries_source}",
        "issues": "${sinoseries_issues}"
    },
    "license": "${sinoseries_license}",
    "icon": "sinobrush_logo.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "games.moegirl.sinocraft.sinobrush.fabric.SinoBrushFabric"
        ],
        "client": [
            "games.moegirl.sinocraft.sinobrush.fabric.SinoBrushFabricClient"
        ]
    },
    "mixins": [
        "sinobrush-common.mixins.json"
    ],
    "depends": {
        "java": "${fabric_java_version_range}",
        "minecraft": "${fabric_minecraft_version_range}",
        "fabricloader": "${fabric_loader_version_range}",
        "fabric-api": "${fabric_api_version_range}",
        "sinocore": "${fabric_sinoseries_version_range}"
    }
}